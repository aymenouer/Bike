{% extends "temp_admin.html.twig" %}
{% block content %}
    <div class="row mt">
        <div class="col-md-12">
            <div class="content-panel">
                <table class="table table-striped table-advance table-hover" id="tableac">
                    <h4><i class="fa fa-angle-right"></i> Participation Evenements</h4>
                    <hr>
                    <thead>
                    <tr>
                        <th><i class="fa fa-bullhorn"></i> id</th>

                        <th><i class="fa fa-bullhorn"></i> Evenement</th>
                        <th class="hidden-phone"><i class="fa fa-question-circle"></i> Client</th>

                        <th></th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for e in event %}
                        <tr>


                            <td>{{ e.id }} </td>
                            <td>{{ e.idevenement.titre }}</td>
                            <td>{{ e.idUser.username }}</td>

                        </tr>
                    {% endfor %}


                    </tbody>
                </table>
            </div>
            <!-- /content-panel -->
        </div>
        <!-- /col-md-12 -->
    </div>

{% endblock %}